//Googler_adarsh
#include<bits/stdc++.h>
#include<map>
#define ll long long
using namespace std; 
ll celling(ll temp[9999],ll len,ll arr[9999],ll src){
	ll strt=0,end=len;
	ll mid=0;
	while(strt<=end)
	{
		mid=(strt+end)/2;
		if(mid<len && arr[temp[mid]]<src && src<=arr[temp[mid+1]])
		{
			return mid+1;
		}
		else if(arr[temp[mid]]<src){
			strt=mid+1;
		}
		else{
			end=mid-1;
		}
	}
	return -1;
}
ll lis(ll arr[9999],ll n){
	ll temp[n+9],result[n+9];
	ll len=0;
	for(ll i=0;i<n;i++)
		result[i]=-1;
	temp[0]=0;
	for(ll i=1;i<n;i++)
	{      
		if(arr[temp[0]]>arr[i])
		{
			temp[0]=i;
		}
		else if(arr[temp[len]]<arr[i])
		{
			len++;
			temp[len]=i;
			result[temp[len]]=temp[len-1];
		}
		else{
			ll u=celling(temp,len,arr,arr[i]);
			temp[u]=i;
			result[temp[u]]=temp[u-1];
		}
	}
	ll index=temp[len];
	cout<<len+1<<endl;
}
int main(){
	ll t;
	cin>>t;
	while(t--)
	{
		ll n,arr[9999];
		cin>>n;
		for(ll i=0;i<n;i++)
			cin>>arr[i];
		lis(arr,n);
	}
	return 0;
}
